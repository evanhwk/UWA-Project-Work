        SRC_INFOBOX:    window.location.protocol == 'https:' ? 'http://web.archive.org/web/20160903081854/https://static.weboffice.uwa.edu.au/visualid/scripts/gmaps/gmaps-infobox.js' : 'http://web.archive.org/web/20160903081854/http://static.weboffice.uwa.edu.au/visualid/scripts/gmaps/gmaps-infobox.js',

