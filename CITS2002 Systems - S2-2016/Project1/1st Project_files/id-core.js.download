        if ( typeof window.onload != 'function' )
            window.onload = processReadyQueue;

        else {
            var oldFn = window.onload;
            window.onload = function() {
                if (oldFn) {
                    try {
                        oldFn();
                    }
                    catch (e) {
                        return;
                    }
                }
                processReadyQueue();
            }
        }

    } // end bindReady()
    
 
